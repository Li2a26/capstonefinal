       \r\n        </tbody>\r\n  \r\n        <button class=\"checkout\">\r\n          <router-link to=\"/checkout\" @click=\"checkout\" > Checkout </router-link>\r\n        </button>    \r\n      </table>\r\n    </div>\r\n  \r\n      \r\n  \r\n    <!-- </div> -->\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n      Name: 'setCart',\r\n      computed: {\r\n        getCart() {        \r\n          return this.$store.state.cart\r\n        },\r\n        user(){\r\n          console.log(this.$store.state.userData.userID)\r\n          return this.$store.state.userData\r\n        },\r\n        cartTotalPrice() {\r\n        return this.$store.getters.cartTotalPrice;\r\n      },\r\n      \r\n      },\r\n      \r\n      mounted() {\r\n        this.$store.dispatch('getCart', this.user.userID)\r\n      },\r\n      methods: {\r\n  \r\n      removeFromCart(cartID) {\r\n      const userID = this.user.userID;\r\n      \r\n      this.$store.dispatch('removeFromCart', { userID, cartID })\r\n        .then(() => {\r\n          // Optional: Perform any additional actions after successful removal\r\n        })\r\n        .catch((error) => {\r\n          console.error('Error removing item from cart:', error);\r\n        });\r\n    },\r\n    },\r\n  }\r\n  </script>\r\n  <style scoped>\r\n  \r\n  \r\n  .tableImg {\r\n    width: 20%;\r\n  }\r\n  \r\n  .container {\r\n    margin-top: 50px;\r\n  }\r\n  \r\n  table{\r\n    width: 100%;\r\n  }\r\n  \r\n  tbody {\r\n    text-align: center;\r\n    \r\n  }\r\n  \r\n  thead {\r\n    color: #d914e7;\r\n  text-align: center;\r\n  \r\n  font-size: 35px;\r\n  font-weight: 400;\r\n  line-height: normal;\r\n  \r\n  \r\n  }\r\n  \r\n  th, td, tr {\r\n   \r\n    border: 10px solid pink;\r\n  \r\n  }\r\n  \r\n  td {\r\n    font-weight: 600;\r\n    color: pink;\r\n  }\r\n  \r\n  \r\n  \r\n  .checkout {\r\n    margin: 30px 0 30px 0 ;\r\n    \r\n  }\r\n  \r\n  .delete, .checkout {\r\n    background-color: #d914e7;\r\n    color:pink;\r\n  text-align: center;\r\n  font-family: Inter;\r\n  font-size: 20px;\r\n  \r\n  line-height: normal;\r\n  \r\n  border: 20px;\r\n  padding: 3px 3px 3px 3px;\r\n  }\r\n  \r\n  </style>\r\n<!-- <template>\r\n    <div>\r\n      <div v-if=\"cart\" class=\"row p-4 justify-content-evenly\">\r\n        <cart />\r\n      </div>\r\n      <div v-else>Cart is empty.</div>\r\n    </div>\r\n  </template>\r\n  <script>\r\n  import cart from \"@/components/CartComp.vue\";\r\n  export default {\r\n    computed: {\r\n      allCart() {\r\n        return this.$store.state.cart;\r\n      },\r\n      cart() {\r\n        return this.allCart || [];\r\n      },\r\n    },\r\n    // mounted() {\r\n    //   this.$store.dispatch(\"getCart\");\r\n    // },\r\n    mounted() {\r\n      this.$store.dispatch(\"getCart\");\r\n    },\r\n    components: { cart },\r\n  }; -->\r\n  <!-- </script> -->"],"mappings":"AAuCE,eAAe;EACXA,IAAI,EAAE,SAAS;EACfC,QAAQ,EAAE;IACRC,OAAOA,CAAA,EAAG;MACR,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAG;IAC9B,CAAC;IACDC,IAAIA,CAAA,EAAE;MACJC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACL,MAAM,CAACC,KAAK,CAACK,QAAQ,CAACC,MAAM;MAC7C,OAAO,IAAI,CAACP,MAAM,CAACC,KAAK,CAACK,QAAO;IAClC,CAAC;IACDE,cAAcA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACR,MAAM,CAACS,OAAO,CAACD,cAAc;IAC3C;EAEA,CAAC;EAEDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACV,MAAM,CAACW,QAAQ,CAAC,SAAS,EAAE,IAAI,CAACR,IAAI,CAACI,MAAM;EAClD,CAAC;EACDK,OAAO,EAAE;IAETC,cAAcA,CAACC,MAAM,EAAE;MACvB,MAAMP,MAAK,GAAI,IAAI,CAACJ,IAAI,CAACI,MAAM;MAE/B,IAAI,CAACP,MAAM,CAACW,QAAQ,CAAC,gBAAgB,EAAE;QAAEJ,MAAM;QAAEO;MAAO,CAAC,EACtDC,IAAI,CAAC,MAAM;QACV;MAAA,CACD,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBb,OAAO,CAACa,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD,CAAC,CAAC;IACN;EACA;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}